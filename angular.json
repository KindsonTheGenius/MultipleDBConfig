{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "jl-clean": {
      "root": "apps/jl-clean",
      "sourceRoot": "apps/jl-clean/src",
      "projectType": "application",
      "prefix": "pl",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/apps/jl-clean",
            "index": "apps/jl-clean/src/index.html",
            "main": "apps/jl-clean/src/main.ts",
            "polyfills": "apps/jl-clean/src/polyfills.ts",
            "tsConfig": "apps/jl-clean/tsconfig.app.json",
            "assets": [
              "apps/jl-clean/src/favicon.ico",
              "apps/jl-clean/src/assets"
            ],
            "styles": [
              "apps/jl-clean/src/styles.scss"
            ],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/jl-clean/src/environments/environment.ts",
                  "with": "apps/jl-clean/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "jl-clean:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "jl-clean:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jl-clean:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/jl-clean/src/test.ts",
            "polyfills": "apps/jl-clean/src/polyfills.ts",
            "tsConfig": "apps/jl-clean/tsconfig.spec.json",
            "karmaConfig": "apps/jl-clean/karma.conf.js",
            "styles": [
              "apps/jl-clean/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/jl-clean/src/favicon.ico",
              "apps/jl-clean/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/jl-clean/tsconfig.app.json",
              "apps/jl-clean/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "jl-clean-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "jl-clean:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jl-clean:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "jl-clean"
}