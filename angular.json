{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "contact-form": {
      "root": "apps/contact-form",
      "sourceRoot": "apps/contact-form/src",
      "projectType": "application",
      "prefix": "pl",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/apps/contact-form",
            "index": "apps/contact-form/src/index.html",
            "main": "apps/contact-form/src/main.ts",
            "polyfills": "apps/contact-form/src/polyfills.ts",
            "tsConfig": "apps/contact-form/tsconfig.app.json",
            "assets": [
              "apps/contact-form/src/favicon.ico",
              "apps/contact-form/src/assets"
            ],
            "styles": ["apps/contact-form/src/styles.scss"],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/contact-form/src/environments/environment.ts",
                  "with": "apps/contact-form/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "contact-form:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "contact-form:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "contact-form:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/contact-form/src/test.ts",
            "polyfills": "apps/contact-form/src/polyfills.ts",
            "tsConfig": "apps/contact-form/tsconfig.spec.json",
            "karmaConfig": "apps/contact-form/karma.conf.js",
            "styles": ["apps/contact-form/src/styles.scss"],
            "scripts": [],
            "assets": [
              "apps/contact-form/src/favicon.ico",
              "apps/contact-form/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/contact-form/tsconfig.app.json",
              "apps/contact-form/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "contact-form-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "contact-form:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "contact-form:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "order": {
      "projectType": "library",
      "root": "libs/order",
      "sourceRoot": "libs/order/src",
      "prefix": "jl-clean",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/order/tsconfig.lib.json",
              "libs/order/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/order/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/order/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "tour": {
      "projectType": "library",
      "root": "libs/tour",
      "sourceRoot": "libs/tour/src",
      "prefix": "jl-clean",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/tour/tsconfig.lib.json",
              "libs/tour/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/tour/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/tour/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "analytics": {
      "projectType": "library",
      "root": "libs/analytics",
      "sourceRoot": "libs/analytics/src",
      "prefix": "jl-clean",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/analytics/tsconfig.lib.json",
              "libs/analytics/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/analytics/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/analytics/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "defaultProject": "contact-form"
}
