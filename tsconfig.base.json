{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "paths": {
      "@jl-clean/order": ["libs/order/src/index.ts"],
      "@jl-clean/tour": ["libs/tour/src/index.ts"],
      "@jl-clean/analytics": ["libs/analytics/src/index.ts"],
      "@jl-clean/lib/calculator-cushion": [
        "libs/lib/calculator-cushion/src/index.ts"
      ],
      "@jl-clean/libs/calculator-cushion": [
        "libs/libs/calculator-cushion/src/index.ts"
      ],
      "@jl-clean/cushion-calculator": ["libs/cushion-calculator/src/index.ts"],
      "@jl-clean/partner": ["libs/partner/src/index.ts"],
      "@jl-clean/models": ["libs/models/src/index.ts"]
    },
    "rootDir": "."
  }
}
